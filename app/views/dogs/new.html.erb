<h1>Add a new Dog</h1>
<%=form_for @dog do |f|%>
  <%=f.label "name"%>
  <%=f.text_field "name"%>
  <%=f.label "breed"%>
  <%=f.text_field "breed"%>
  <%=f.label "age"%>
  <%=f.text_field "age"%>
  <%=f.label "employee owners:"%>
  <%=f.text_field "employees names"%>
  <%=f.submit "Update"%>
<%end%>
