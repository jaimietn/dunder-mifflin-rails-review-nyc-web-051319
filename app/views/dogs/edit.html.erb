<h1>Dogs Edit</h1>
<%=form_for @dog do |f|%>
  <%=f.label "name"%>
  <%=f.text_field "name"%>
  <%=f.label "breed"%>
  <%=f.text_field "breed"%>
  <%=f.label "age"%>
  <%=f.text_field "age"%>
  <%=f.label "employee owners:"%>
  <%=f.text_field "email"%>
  <%=f.collection_select(:employee_id, @employees, :id, :full_name, prompt:true)%>
  <%=f.submit "Update"%>
<%end%>
